@model List<MemberModel>
@{
    ViewData["Title"] = "Home Page";
}
<main>

    <section>
        <div class="container">

            <h1>scroll down to see the team </h1>
            <p>Welcome to our simple webpage which will show the data of the creator team</p>
            <img class="mainimage" src="~/Assets/Before Dawn-cuate.svg" alt="rest">


        </div>

    </section>

    <div class="sticky_parent">
        <div class="sticky">
            <div class="scroll_section">

                <div class="prof" style="background-color: #35374B;">

                    <div class="flex-container">

                        <div class=" image">
                            <img src="data:image/png;base64, @Model[1].Image" class="clip_mask">
                        </div>
                        <div style="display: flex; justify-content: start; flex-direction: column;">
                            <div class="flex-item name">Name : @Model[1].MemberName</div>


                            <div class="flex-item id">Id :@Model[1].Id </div>
                            <div class="flex-item age">age :@Model[1].Age </div>
                            <div class="flex-item cgpa">cgpa :@Model[1].CGPA </div>
                        </div>

                    </div>

                    <div class="spacer left" style="background-image: url('/Assets/one.svg');"></div>
                </div>

                <div class="prof" style="background-color: #344955;">

